func main() {
    a = -5;
    if(true == a) { print(1); }
    if(a == true) { print(2); }
    if(false == a) { print(3); }
    if(a == false) { print(4); }

    if(true != a) { print(1); }
    if(a != true) { print(2); }
    if(false != a) { print(3); }
    if(a != false) { print(4); }

    if(a) {print(5);}
    if(!a) {print(6);}

    a = 0;

    if(a) {print(7);}
    if(!a) {print(8);}

    if(true + true) { print(9); }
    if(true - true) { print(10); }
    if(true / true) { print(11); }
    if(true * true) { print(12); }

    if(false + false) { print(13); }
    if(false - false) { print(14); }
    if(false * false) { print(15); }

    if(!1) {print("16"); }
    if(!0) {print("17"); }
    if(!0 + 0) {print(18); }

    if(true >= 0) { print(19); }
    if(0 <= true) { print(20); }

    if(true && 1) { print(21); }
    if(1 && true) { print(22); }
    if(true && 0) { print(23); }
    if(0 && true) { print(24); }

    if(false && 1) { print(21); }
    if(1 && false) { print(22); }
    if(false && 0) { print(23); }
    if(0 && false) { print(24); }

    if(true || 1) { print(21); }
    if(1 || true) { print(22); }
    if(true || 0) { print(23); }
    if(0 || true) { print(24); }

    if(false || 1) { print(21); }
    if(1 || false) { print(22); }
    if(false || 0) { print(23); }
    if(0 + 1 || false) { print(24); }

}

