func main() {
  b = true + 123;
  print(b);
  
  b = 123 + true;
  print(b);
  
  b = 123 + false;
  print(b);

  b = 123 - true;
  print(b);

  b = 123 * false;
  print(b);

  b = false * 123;
  print(b);

  c = true + true;
  print(c);
  
  d = false + true;
  print(d);
}

