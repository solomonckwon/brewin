
func main() {
    a = lambda() { print("original"); };
    /* saves environment where a prints original */


    x = lambda (ref f) {
      f = lambda() { print("modified"); };
      a();
      f();
      /* this still prints original, despite assignment */

      print(f == a);
    };
    /* saves environment where x a prints original */
    
    x(a);
    a();
}

